{
  "name": "w3name",
  "version": "1.0.4",
  "description": "The JavaScript API client w3name",
  "author": "",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/web3-storage/w3name#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/web3-storage/w3name/tree/main/packages/client"
  },
  "bugs": {
    "url": "https://github.com/web3-storage/w3name/issues"
  },
  "keywords": [
    "w3name",
    "ipns"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "aegir": {
    "tsRepo": true
  },
  "ts-standard": {
    "ignore": [
      "/dist/"
    ]
  },
  "directories": {
    "test": "test"
  },
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./service": {
      "types": "./dist/src/service.d.ts",
      "import": "./dist/service.mjs",
      "require": "./dist/service.cjs"
    }
  },
  "module": "dist/index.mjs",
  "typesVersions": {
    "*": {
      "index": [
        "dist/src/index.d.ts"
      ],
      "service": [
        "dist/src/service.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "rollup --config rollup.cjs.config.js",
    "build:esm": "rollup --config rollup.esm.config.js",
    "build:tsc": "tsc --build",
    "lint": "aegir lint",
    "publish": "npm run lint && npm run test && npm run build && npm publish && npm run typedoc:publish",
    "test": "run-p -r test:all",
    "test:all": "npm run build && API_PORT=1337 aegir test --target node",
    "test:ts": "mocha -r ts-node/register --loader=ts-node/esm test/name.spec.ts",
    "typedoc": "typedoc",
    "typedoc:publish": "npm run typedoc && gh-pages -d generated-docs"
  },
  "dependencies": {
    "@web-std/fetch": "^4.1.0",
    "cborg": "^1.9.4",
    "ipns": "^0.16.0",
    "libp2p-crypto": "^0.21.2",
    "throttled-queue": "^2.1.4",
    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/mocha": "^9.1.1",
    "aegir": "^37.3.0",
    "gh-pages": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.78.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.10",
    "typings": "^2.1.1",
    "uint8arrays": "^3.0.0"
  }
}
